{
    "manifest_version": 2,
    "name": "Page Downloader",
    "version": "1.0",

    "description": "Dowloads pages in background.",

    "background": {
        "scripts": ["download_page.js"]
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["get_page.js"]
        }
    ],

    "permissions": [
        "tabs",
		"downloads",
		"<all_urls>"
    ]
}